            <Font name="Georgia Bold Italic" size="20.0" />
         </font>
      </Button>
      <Button fx:id="logOut_btn" layoutX="1192.0" layoutY="62.0" mnemonicParsing="false" onAction="#LogOut" prefHeight="30.0" prefWidth="100.0" styleClass="pink-btn" stylesheets="@application.css" text="Log Out" textFill="#901b1b">
         <font>
            <Font name="Georgia Bold Italic" size="15.0" />
         </font>
      </Button>
   </children>
