<AnchorPane prefHeight="800.0" prefWidth="1306.0" styleClass="pane" stylesheets="@application.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="todoListController.TaskManagerController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Task" textFill="#b8e994">
         <font>
            <Font name="Bauhaus 93" size="55.0" />
         </font>
      </Label>
      <Label layoutX="125.0" layoutY="14.0" text=" Manager" textFill="#78e08f">
         <font>
            <Font name="Bauhaus 93" size="55.0" />
         </font>
      </Label>
      <Pane layoutX="14.0" layoutY="85.0" prefHeight="17.0" prefWidth="344.0" style="-fx-background-color: #82ccdd; -fx-background-radius: 10;" />
      <TableView fx:id="table" layoutX="14.0" layoutY="131.0" prefHeight="647.0" prefWidth="1278.0" styleClass="tf" stylesheets="@application.css" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0">
        <columns>
          <TableColumn fx:id="title_col" prefWidth="254.0" text="Title" />
